    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.45",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@unocss/preset-icons": "^0.46.4",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
