{
  "name": "uniapp-vue-next-starter",
  "version": "1.0.0",
  "scripts": {
    "dev:app": "uni -p app",
    "dev:h5": "uni",
    "dev:mp-weixin": "uni -p mp-weixin",
    "build:app": "uni build -p app",
    "build:h5": "uni build --minify",
    "build:mp-weixin": "uni build -p mp-weixin --minify",
    "lint": "eslint --cache --max-warnings 0  \"src/**/*.{js,jsx,vue,ts,tsx}\"",
    "lint:fix": "eslint --cache --max-warnings 0  \"src/**/*.{js,jsx,vue,ts,tsx}\" --fix",
    "prepare": "husky install",
    "hbx:open": "cli open",
    "hbx:custom": "node ./scripts/hbx.pack.js custom",
    "hbx:pack": "node ./scripts/hbx.pack.js",
    "uniapp-cli:latest": "npx @dcloudio/uvm"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-3070920230324001",
    "@dcloudio/uni-app-plus": "3.0.0-3070920230324001",
